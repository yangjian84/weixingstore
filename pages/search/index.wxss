page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row input {
  flex: 1;
  height: 100%;
  padding-left: 20rpx;
  background-color: #fff;
}
.search_row button {
  width: 110rpx;
  height: 100%;
  font-size: 28rpx;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 10rpx;
}
.search_content {
  margin-top: 20rpx;
}
.search_content .search_item {
  background-color: #fff;
  padding: 20rpx 10rpx;
  border-bottom: 1px solid #D7D7D4;
  font-size: 26rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
